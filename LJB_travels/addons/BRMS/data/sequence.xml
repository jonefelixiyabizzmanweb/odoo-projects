<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data >
        <!-- Sequences for lead mining requests -->
<!--       bus -->
        <record id="sequence_id" model="ir.sequence">
            <field name="name">Sequence Id</field>
            <field name="code">BRMS.bus.details</field>
            <field name="prefix">BUS</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
<!--        customer-->
         <record id="sequence_id_1" model="ir.sequence">
